@page
@model empdetailsWebApp.Pages.EmpDetails.DeleteModel
@{
}
<br />
<h2>Delete Data</h2>

@if (@Model.errorMessage.Length > 0)
{
    <div>@Model.errorMessage</div>
}
<form method="POST">
    <input type="hidden" name="id" value="@Model.emp.id" />
    <table>
        <tr>
            <td>work_year</td>
            <td>@Model.emp.work_year</td>
        </tr>
        <tr>
            <td>experience_level</td>
            <td>@Model.emp.experience_level</td>
        </tr>
        <tr>
            <td>job_title</td>
            <td>@Model.emp.job_title</td>
        </tr>
        <tr>
            <td>salary_in_usd</td>
            <td>@Model.emp.salary_in_usd</td>
        </tr>
        <tr>
            <td>employee_residence</td>
            <td>@Model.emp.employee_residence</td>
        </tr>
        <tr>
            <td>company_location</td>
            <td>@Model.emp.company_location</td>
        </tr>
        <tr>
            <td>company_size</td>
            <td>@Model.emp.company_size</td>
        </tr>
    </table>
    <p></p>
    @if (@Model.successMessage.Length > 0)
    {
        <div>@Model.successMessage</div>
    }
    <input type="submit" value="Delete" class="btn btn-danger" />
</form>
